{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefintion-Prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "506732059628.dkr.ecr.us-east-1.amazonaws.com/learnjenkinsapp:#APP_VERSION#",
            "portMappings": [
                {
                    "name": "nginx-80-tip",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": ".25 vCPU",
    "executionRoleArn": "arn:aws:iam::506732059628:role/ecsTaskExecutionRole"
}